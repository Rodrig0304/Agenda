GET     /                 controllers.HomeController.index

GET     /assets/*file       controllers.Assets.versioned(path="/public", file: Asset)

GET     /contactos         controllers.ContactoController.listarContactos
POST    /contactos         controllers.ContactoController.crearContacto(request: play.mvc.Http.Request)

GET     /categorias        controllers.CategoriaController.listarCategorias
POST    /categorias        controllers.CategoriaController.crearCategoria(request: play.mvc.Http.Request)

GET     /eventos          controllers.EventoController.listarEventos
POST    /eventos          controllers.EventoController.crearEvento(request: play.mvc.Http.Request)

GET     /vista/categorias           controllers.CategoriaController.vistaCategorias
POST    /vista/categorias/crear     controllers.CategoriaController.crearDesdeFormulario(request: play.mvc.Http.Request)

GET     /vista/eventos              controllers.EventoController.vistaEventos
POST    /vista/eventos/crear        controllers.EventoController.crearDesdeFormulario(request: play.mvc.Http.Request)

GET     /vista/contactos           controllers.ContactoController.vistaContactos
POST    /vista/contactos/crear     controllers.ContactoController.crearDesdeFormulario(request: play.mvc.Http.Request)

